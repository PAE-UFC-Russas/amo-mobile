{
  "expo": {
    "name": "AMO",
    "slug": "ambiente-de-monitoria-online",
    "version": "1.0.0",
    "userInterfaceStyle": "light",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "backgroundColor": "#52D6FB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive_icon.png",
        "backgroundColor": "#52D6FB"
      },
      "package": "com.heronn.ambientedemonitoriaonline"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir que o aplicativo AMO acesse sua galeria de fotos.",
          "cameraPermission": "Permitir que o aplicativo AMO acesse sua camera.",
          "microphonePermission": false
        }
      ]
    ]
  }
}
